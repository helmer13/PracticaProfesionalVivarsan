<Window x:Class="PracticaProfesionalVivarsan.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaProfesionalVivarsan"
        mc:Ignorable="d"
        Title="Iniciar Sesión" Height="374" Width="587">
    <Grid Margin="0,0,0,-4">
        <Button x:Name="btnIngresar" Content="Ingresar" HorizontalAlignment="Left" Margin="260,222,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="238,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox x:Name="txtContrasenna" HorizontalAlignment="Left" Height="23" Margin="238,173,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblUsuario" Content="Nombre Usuario" HorizontalAlignment="Left" Margin="136,110,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblContrasenna" Content="Contraseña" HorizontalAlignment="Left" Margin="136,173,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.976,1.797"/>

    </Grid>
</Window>
